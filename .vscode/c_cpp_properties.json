{
    "version": 4,
    "env": {
        "TCC-65816": [
            "${workspaceFolder}/pvsneslib/compiler/tcc-65816/**"
        ]
    },
    "configurations": [
        {
            "name": "TCC-65816",
            "compilerArgs": [
                "-Wall",
                "-O2",
                "-g",
                "-Wno-pointer-sign",
                "-Wno-maybe-uninitialized",
                "-Wno-unused-but-set-variable",
                "-Wno-format-overflow",
                "-Wno-array-bounds",
                "-Wno-format-truncation",
                "-m32",
                "-mpreferred-stack-boundary=4",
                "-falign-functions=0",
                "-fno-strict-aliasing",
                "-DTCC_TARGET_816"
            ],
            "includePath": [
                "${TCC-65816}"
            ],
            "browse": {
                "limitSymbolsToIncludedHeaders": true,
                "databaseFilename": "",
                "path": [
                    "${TCC-65816}"
                ]
            }
        }
    ]
}